<?xml version="1.0" encoding="UTF-8"?>
<OpenSearchDescription xmlns="http://a9.com/-/spec/opensearch/1.1/">
    <ShortName>Qwant</ShortName> 
    <Description>Qwant</Description> 
    <InputEncoding>UTF-8</InputEncoding> 
    <LongName>Qwant</LongName> 
    <Language>*</Language>
    <Image width="16" height="16">data:image/png;base64,</Image>
    <Url type="text/html" method="GET" template="https://www.qwant.com">
        <Param name="q" value="{searchTerms}"/>
        <Param name="client" value="opensearch"/>
    </Url>
    <Url type="application/x-suggestions+json" method="GET" template="https://api.qwant.com/v3/suggest/">
        <Param name="q" value="{searchTerms}"/>
        <Param name="client" value="opensearch"/>
    </Url>
</OpenSearchDescription>
