<?xml version="1.0" encoding="UTF-8"?>
<OpenSearchDescription xmlns="http://a9.com/-/spec/opensearch/1.1/">
    <ShortName>Startpage</ShortName> 
    <Description>Startpage</Description> 
    <InputEncoding>UTF-8</InputEncoding> 
    <LongName>Startpage</LongName> 
    <Language>*</Language>
    <Image height="16" width="16">https://www.startpage.com/sp/cdn/favicons/apple-touch-icon-57x57--default.png</Image>
    <Url type="text/html" method="GET" template="https://www.startpage.com/sp/search">
        <Param name="query" value="{searchTerms}"/>
        <Param name="cat" value="web"/>
        <Param name="pl" value="opensearch"/>
        <Param name="language" value="english"/>
    </Url>
    <Url type="application/x-suggestions+json" method="GET" template="https://www.startpage.com/suggestions">
        <Param name="q" value="{searchTerms}"/>
        <Param name="format" value="opensearch"/>
    </Url>
</OpenSearchDescription>
